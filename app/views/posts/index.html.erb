<p id="notice"><%= notice %></p>
<div class="box">

<%= render 'layouts/listleft' %>

<div class="list-posts">
<% @posts.each do |post| %>
  <div class="post">
   <h3><%= post.title %></h3>
  <em> creer: <%= post.created_at %></em>
    <div class="buttons">
      <div class="buttons-left">
        <%= link_to "visite-it", url_with_protocol(post.url), target:"_blank", class: "btn btn-primary" %> 
      </div>
      <div class="buttons-right">
        <%= link_to 'Edit', edit_post_path(post) %> |
        <%= link_to 'Destroy', post, method: :delete, data: { confirm: 'Are you sure?' } %>
      </div>
    </div>
  </div> 
<% end %>  
</div>
<div class="list-right">
  information about you
</div>
</div>

